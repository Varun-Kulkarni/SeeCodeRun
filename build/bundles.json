{
  "bundles": {

    "dist/app-build": {
      "includes": [
        "text",
        "jquery",
        "jquery-ui",
        "styles/**/*.gif",
        "styles/**/*.png",
        "jquery-ui/themes/smoothness/*.css!text",
        "bootstrap",
        "bootstrap/css/bootstrap.css!text",
        "**/*.js",
        "**/*.html!text",
        "**/*.css!text"
      ],
      "options": {
        "inject": true,
        "minify": true,
        "depCache": true,
        "rev": true
      }
    },
    "dist/aurelia-build": {
      "includes": [
        "aurelia-framework",
        "aurelia-bootstrapper",
        "aurelia-fetch-client",
        "aurelia-router",
        "aurelia-animator-css",
        "aurelia-templating-binding",
        "aurelia-templating-resources",
        "aurelia-templating-router",
        "aurelia-loader-default",
        "aurelia-history-browser",
        "aurelia-logging-console",
        "aurelia-computed"
      ],
      "options": {
        "inject": true,
        "minify": true,
        "depCache": true,
        "rev": true
      }
    }
  }
}
